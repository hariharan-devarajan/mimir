set(MIMIR_FOUND TRUE)
# Include directories
set(MIMIR_INCLUDE_DIRS "@CMAKE_INCLUDE_OUTPUT_DIRECTORY@")
if (NOT IS_DIRECTORY "${MIMIR_INCLUDE_DIRS}")
    set(MIMIR_FOUND FALSE)
endif ()
#message(STATUS "MIMIR_INCLUDE_DIRS: " ${MIMIR_INCLUDE_DIRS})
get_filename_component(MIMIR_ROOT_DIR ${MIMIR_INCLUDE_DIRS}/.. ABSOLUTE)
#message(STATUS "MIMIR_ROOT_DIR: " ${MIMIR_ROOT_DIR})
set(MIMIR_LIBRARY_PATH "@CMAKE_LIBRARY_OUTPUT_DIRECTORY@")
#message(STATUS "MIMIR_LIBRARY_PATH: " ${MIMIR_LIBRARY_PATH})
set(MIMIR_LIBRARIES "-L${MIMIR_LIBRARY_PATH} -lmimir")
set(MIMIR_DEFINITIONS "")
if (NOT TARGET mimir::mimir)
    add_library(mimir::mimir ALIAS mimir)
    include(FindPackageHandleStandardArgs)
    find_package_handle_standard_args(mimir
            REQUIRED_VARS MIMIR_FOUND MIMIR_INCLUDE_DIRS MIMIR_LIBRARIES)
endif ()